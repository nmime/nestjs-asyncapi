{
  "name": "@nmime/nestjs-asyncapi",
  "version": "2.0.8",
  "description": "NestJS AsyncAPI module - generate documentation of your event-based services using decorators. Supports AsyncAPI 3.0, 2.x, and 1.x specifications.",
  "author": "nmime",
  "contributors": [
    {
      "name": "Ilya Moroz",
      "url": "https://github.com/flamewow"
    }
  ],
  "license": "MIT",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmime/nestjs-asyncapi.git"
  },
  "homepage": "https://github.com/nmime/nestjs-asyncapi#readme",
  "bugs": {
    "url": "https://github.com/nmime/nestjs-asyncapi/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "validate-branch-name": {
    "pattern": "^(main|dev){1}$|^(feat|fix|release|test|refactor|docs|perf|style|chore)/.+$",
    "errorMsg": "The branch name isn't valid."
  },
  "keywords": [
    "asyncapi",
    "asyncapi-3.0",
    "asyncapi-2.0",
    "nest",
    "nestjs",
    "swagger",
    "openapi",
    "documentation",
    "socket.io",
    "websockets",
    "pubsub",
    "microservices",
    "event-driven",
    "kafka",
    "amqp",
    "rabbitmq"
  ],
  "peerDependencies": {
    "@nestjs/common": "^11.0.0 || ^10.0.0 || ^9.0.0",
    "@nestjs/core": "^11.0.0 || ^10.0.0 || ^9.0.0",
    "@nestjs/swagger": "^11.0.0 || ^8.0.0 || ^7.0.0 || ^6.0.0",
    "@nestjs/websockets": "^11.0.0 || ^10.0.0 || ^9.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/websockets": {
      "optional": true
    }
  },
  "dependencies": {
    "@asyncapi/generator": "^3.1.0",
    "@asyncapi/html-template": "^3.5.0",
    "js-yaml": "4.1.1",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "0.2.2"
  },
  "overrides": {
    "tar": "^7.5.3",
    "undici": "^7.18.2",
    "diff": "^8.0.3",
    "markdown-it": "^14.1.0",
    "jsonpath-plus": "^10.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.16",
    "@nestjs/common": "11.1.12",
    "@nestjs/core": "11.1.12",
    "@nestjs/microservices": "11.1.12",
    "@nestjs/platform-express": "11.1.12",
    "@nestjs/platform-fastify": "11.1.12",
    "@nestjs/platform-socket.io": "11.1.12",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/swagger": "11.2.5",
    "@nestjs/testing": "11.1.12",
    "@nestjs/websockets": "11.1.12",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.19.7",
    "@types/supertest": "6.0.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.0",
    "release-it": "19.2.3",
    "socket.io": "4.8.3",
    "socket.io-client": "4.8.3",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepare": "husky",
    "format": "prettier --write \"lib/**/*.{js,ts,json}\"  \"e2e/**/*.{js,ts,json}\"",
    "lint": "eslint --fix \"lib/**/*.ts\"  \"sample/**/*.ts\" \"test/**/*.ts\"",
    "pre-commit": "lint-staged",
    "prestart": "npm install && npm run build",
    "make:snapshots": "bash misc/take-snaphots.sh",
    "---execution---": "",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --watch --debug",
    "---tests---": "",
    "test:e2e": "jest --runInBand --config test/configs/jest-e2e.config.ts",
    "---npm---": "",
    "publish:next": "npm publish --access public --tag next",
    "publish:beta": "npm publish --access public --tag beta",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  }
}
